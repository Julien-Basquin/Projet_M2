	Source = {Scopus},
	Title = {A multiagent knowledge and information network approach for managing research assets},
	Url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-77950188974&doi=10.1016%2fj.eswa.2010.01.012&partnerID=40&md5=40e50f81516b2f68d382c288b7f36668},
