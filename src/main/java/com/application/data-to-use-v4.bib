	Author = {Samper Zapater, J. Javier and Llido Escriva, Dolores M. and Soriano Garcia, Francisco R. and Martinez Dura, Juan Jose},
	Author-Email = {jsamper@irtic.uv.es dlliido@uji.es francisco.r.soriano@uv.es juan.martinez-dura@uv.es},
