	Note = {cited By 8},
	Number = {1},
	Pages = {74-93},
	Publisher = {Institute of Electrical and Electronics Engineers Inc.},
	Source = {Scopus},
	Title = {Sentiment Analysis: From Opinion Mining to Human-Agent Interaction},
	Url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84946927700&doi=10.1109%2fTAFFC.2015.2444846&partnerID=40&md5=5f7b2016eb7daa101b58d1ee40635356},
