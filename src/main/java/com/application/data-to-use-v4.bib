	Journal = {IEEE Transactions on Signal Processing},
	Note = {cited By 18},
	Number = {5},
	Pages = {1165-1170},
	Source = {Scopus},
	Title = {Decentralized jointly sparse optimization by reweighted ℓq minimization},
