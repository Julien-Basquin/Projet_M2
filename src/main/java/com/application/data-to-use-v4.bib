@conference{ZirkelWirtz2010,
	Abstract = {By using the remote functions of a modern IT service management system infrastructure, it is possible to analyze huge amounts of log file data from complex technical equipment. This enables a service provider to predict failures of connected equipment before they happen. The problem most providers face in this context is finding "a needle in a haystack" - the obtained amount of data turns out to be too large to be analyzed manually. The following report describes a process to find suitable predictive patterns in log files for the detection of upcoming critical situations. The identification process may serve as a hands-on guide. It describes how to connect statistical means, data mining algorithms and expert domain knowledge in the domain of service management. The process was developed in a research project which is currently being carried out within the Siemens Healthcare service organization. The project deals with two main aspects: first, the identification of predictive patterns in existing service data and second, the architecture of an autonomous agent which is able to correlate such patterns. This paper summarizes the results of the first project challenge. The identification process was tested successfully in a proof of concept for several Siemens Healthcare products. {\copyright}2010 IEEE.},
	Art_Number = {5530120},
	Author = {Zirkel, W. and Wirtz, G.},
	Author_Keywords = {Event correlation; Pattern identification; Service management},
	Document_Type = {Conference Paper},
	Doi = {10.1109/ICSSSM.2010.5530120},
	Journal = {2010 7th International Conference on Service Systems and Service Management, Proceedings of ICSSSM' 10},
	Note = {cited By 0},
	Pages = {885-890},
	Source = {Scopus},
	Title = {A process for identifying predictive correlation patterns in service management systems},
	Url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-77955938134&doi=10.1109%2fICSSSM.2010.5530120&partnerID=40&md5=a77d67b8b8616f2b40e3783bca667e27},
