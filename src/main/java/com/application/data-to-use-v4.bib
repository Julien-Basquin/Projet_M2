	Source = {Scopus},
	Title = {Enhancing the effectiveness of Ant Colony Decision Tree algorithms by co-learning},
	Url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84922726367&doi=10.1016%2fj.asoc.2014.12.036&partnerID=40&md5=a3713acd5928eb7d91903bb2bf962b8b},
