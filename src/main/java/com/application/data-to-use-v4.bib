	Journal = {Conference Proceedings - IEEE International Conference on Systems, Man and Cybernetics},
	Note = {cited By 6},
	Pages = {1863-1870},
	Source = {Scopus},
	Title = {Improving reinforcement learning algorithms by the use of data mining techniques for feature and action selection},
