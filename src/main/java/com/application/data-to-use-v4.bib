	Bdsk-Url-2 = {http://dx.doi.org/10.1007/s10458-011-9189-y}}

@conference{EissaEl-Sharkawi2007,
	Abstract = {Protein-protein interaction (PPI) is the core process of almost all vital processes for living organisms. Therefore, PPI prediction is very important for many disciplines, e.g. drug and vaccine discovery and development. Exhaustive lab experimentation of every possible pairwise interaction among proteins of a given proteome is not a feasible solution due to its very high cost and long experimentation time. In this paper, we introduce an intelligent framework for predicting protein-protein interactions. The framework consists of five components that are integrated to produce PPI predictions; Database, Knowledgebase, Data mining, Agent-based Simulation and Visualization components. The framework produces a variety of outputs including predicted multilevel, multidimensional association rules governing PPI, predicted protein subnet for any protein of interest (in the form of an interaction network) and also a list of pairwise predicted interactions among proteins of the organism of study. In contrast with other approaches that tend to make cross-organism (cross-species) comparisons, our framework is organism-specific and considers multiple features of the proteins as parameters to the prediction mechanism which is expected to increase the quality of the produced predictions.},
	Author = {Eissa, A.H.B. and El-Sharkawi, M.E.},
	Author_Keywords = {Association Rule; Bioinformatics; Data mining; Protein-protein interactions; Proteomics},
	Document_Type = {Conference Paper},
